  pinMode(btn_DOWN, INPUT_PULLUP);
  pinMode(btn_TRIGGER, INPUT_PULLUP);
  pinMode(out_led_Logo,  OUTPUT);
  pinMode(out_TEMP, OUTPUT);
  pinMode(out_pwm_FAN, OUTPUT);
  pinMode(out_led_band, OUTPUT);
  pinMode(out_loadBatery, OUTPUT);
  pinMode(out_Test, OUTPUT);
  // set OLED
  myOLED.begin();//inicializa el display OLED
  myOLED.rotateDisplay(true); // rotación de la pantalla

  digitalWrite(out_led_Logo, 0); 
  digitalWrite(out_TEMP, 0); 
  analogWrite(out_pwm_FAN, 0); 
  digitalWrite(out_led_band, 0); 
  digitalWrite(out_loadBatery, 0); 
  digitalWrite(out_Test, 0); 
}

void loop() {

  if(appOn){
    logicaPistolaOn();
  }else if(query_state_inputDigital(btn_inSetup)){
    checkOnOf();
  }
  
}

void checkOnOf(){ // revisa si se va encender o apagar la pistola 
  int contOnOf = 0;
  while(query_state_inputDigital(btn_inSetup) == true){
    contOnOf ++;
    delay(1000); // incrementa el contOnOf cada segundo
    if(contOnOf > 2){
      appOn = !appOn;
      if(appOn){
        welcomeScreem();
      }else{
        goodByScreem();
      }
      
    }
  }
}

